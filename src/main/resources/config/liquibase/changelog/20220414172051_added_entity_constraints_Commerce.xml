<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <!--
        Added the constraints for entity Commerce.
    -->
    <changeSet id="20220414172051-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="commerce_id"
                                 baseTableName="rel_commerce__cooperative"
                                 constraintName="fk_rel_commerce__cooperative__commerce_id"
                                 referencedColumnNames="id"
                                 referencedTableName="commerce"/>

        <addForeignKeyConstraint baseColumnNames="cooperative_id"
                                 baseTableName="rel_commerce__cooperative"
                                 constraintName="fk_rel_commerce__cooperative__cooperative_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cooperative"/>

        <addForeignKeyConstraint baseColumnNames="utilisateur_id"
                                 baseTableName="commerce"
                                 constraintName="fk_commerce__utilisateur_id"
                                 referencedColumnNames="id"
                                 referencedTableName="utilisateur"/>
    </changeSet>
</databaseChangeLog>
